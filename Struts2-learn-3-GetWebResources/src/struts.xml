<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="GetWebResources" extends="struts-default">
		<action name="TestActionContext" class="cn.llanc.GetWebResources.TestActionContextAction">
			<result>/WEB-INF/pages/test-ActionContext.jsp</result>
		</action>
		<action name="TestAware" class="cn.llanc.GetWebResources.TestAwareAction">
			<result>/WEB-INF/pages/test-AwareAction.jsp</result>
		</action>
		<action name="TestServletActionContext" class="cn.llanc.GetWebResources.TestServletActionContextAction">
			<result>/WEB-INF/pages/test-ServletActionContext.jsp</result>
		</action>
		<action name="TestServletAware" class="cn.llanc.GetWebResources.TestServletAwareAction">
			<result>/WEB-INF/pages/test-ServletAware.jsp</result>
		</action>
	</package>

</struts>
